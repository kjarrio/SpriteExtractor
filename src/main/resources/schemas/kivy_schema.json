{
  "$schema": "http://json-schema.org/draft-07/schema",
  "$id": "https://raw.githubusercontent.com/kjarrio/SpriteExtractor/main/src/main/resources/schemas/kivy_schema.json",
  "type": "object",
  "patternProperties": {
    "^.*$": {
      "type": "object",
      "patternProperties": {
        "^.*$": {
          "type": "array",
          "minItems": 4,
          "maxItems": 4,
          "items": {"type": "integer"},
          "additionalItems": false
        }
      }
    }
  }
}